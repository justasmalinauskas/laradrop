{
    "name": "jasekz/laradrop",
    "description": "File manager using Dropzone.js for Laravel 5 | 6 | 7 | 8 | 9 | 10",
    "keywords": [
        "php",
        "laravel 5",
        "laravel 6",
        "laravel 7",
        "laravel 8",
        "laravel 9",
        "laravel 10",
        "dropzone.js",
        "file manager",
        "file upload"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jasek Zbigniew",
            "email": "zig@elegrit.com",
            "homepage": "http://elegrit.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "ext-fileinfo": "*",
        "intervention/image": "2.*",
        "illuminate/database": "~5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "ext-json": "*",
        "illuminate/routing": "~5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/queue": "~5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
    },
    "autoload": {
        "psr-0": {
            "Jasekz\\Laradrop": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jasekz\\Laradrop\\LaradropServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
